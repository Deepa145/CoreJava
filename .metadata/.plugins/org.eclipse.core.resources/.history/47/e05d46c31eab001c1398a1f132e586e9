package com.chainsys.firstlesson;

public class Ifmethod {

	public static void main(String[] args)
	   {
		// TODO Auto-generated method stub
          printNumberOneToHunderd();
          matrix();
	   }
    public static void printNumberOneToHunderd()
	   {
	int count=0;
	for(count=0;count<=100;count++)
	{
		System.out.print(count +" ");
	}

}
    public static void matrix()
    {
    int countA=0;
    for(countA=1;countA<=10;countA++)
    {
    	for(int countB=1;countB<=10;count++)
    	{
    		System.out.print(false)countB+" ");
    	}
    	System.out.println();
    	}
    }
    
}

